   button-text=\"提交订单\"\n      @submit=\"onSubmit\"\n      button-type=\"info\"\n    />\n  </div>\n</template>\n<script>\nimport Blue from \"./blue.png\";\nexport default {\n  data() {\n    return {\n      Blue,\n      username: \"\",\n      password: \"\",\n      list: \"\"\n    };\n  },\n  methods: {\n    onSubmit() {},\n    onClickLeft() {}\n  },\n  created() {\n    let str = window.location.hash;\n    let index = str\n      .split(\"/\")[1]\n      .split(\"?\")[1]\n      .split(\"=\")[1];\n    console.log(index);\n    console.log(this.$store.state);\n    let res = this.$store.state.alllists[index];\n    this.list = res;\n  }\n};\n</script>\n<style scoped>\n.mine {\n  height: 100%;\n  background: #f2f2f2;\n}\n.mine .bjpic {\n  position: absolute;\n  top: 0;\n  height: 4rem;\n  width: 100%;\n}\n\n.mine .van-nav-bar {\n  background: #4aa6f8;\n}\n.mine .van-nav-bar__title {\n  color: #fff;\n  font-weight: 700;\n}\n.mine .van-icon-arrow-left {\n  color: white;\n}\n.mine .orderlist {\n  z-index: 999;\n  /* background: #fff; */\n  width: 95%;\n  height: 80%;\n  position: absolute;\n  top: 1.3rem;\n  left: 0.2rem;\n}\n.mine .orderlist .list1Top img {\n  width: 2.26667rem;\n  height: 2.26667rem;\n  float: left;\n}\n.mine .orderlist .list1Top {\n  height: 3rem;\n}\n.mine .orderlist .list1 {\n  background: #fff;\n  height: 5rem;\n  width: 100%;\n  padding: 0.2rem 0.3rem;\n  border-radius: 0.2rem;\n}\n.mine .orderlist .list1 h4 {\n  float: left;\n  width: 65%;\n  margin-left: 0.6rem;\n  font-weight: 500;\n  margin-top: 0.3rem;\n}\n.mine .orderlist .list1 span:nth-of-type(1) {\n  float: left;\n  margin-left: 0.6rem;\n}\n.mine .orderlist .list1 span:nth-of-type(2) {\n  float: right;\n}\n.mine .orderlist .list1 .list1Bottom {\n  width: 70%;\n  height: 1rem;\n  margin: auto;\n  background: #fbf8dc;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  color: #d07234;\n  font-size: 0.32rem;\n  padding: 0.1rem 0.2rem;\n}\n.mine .orderlist .list2 {\n  margin-